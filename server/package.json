{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/server/test/jest-e2e.json"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.1.2",
    "@nestjs-modules/mailer": "1.3.22",
    "@nestjs/bull": "^0.1.2",
    "@nestjs/common": "^7.3.1",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.3.1",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.3.1",
    "@nestjs/swagger": "^4.5.12",
    "@typegoose/typegoose": "^7.2.0",
    "@types/mongoose": "^5.7.29",
    "@types/uuid": "^8.0.0",
    "aws-sdk": "^2.717.0",
    "bcryptjs": "^2.4.3",
    "bull": "^3.15.0",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "handlebars": "^4.7.6",
    "mongoose": "^5.9.21",
    "multer-aliyun-oss": "^1.1.0",
    "nest-access-control": "^2.0.2",
    "nestjs-mongoose-crud": "^2.0.0",
    "nestjs-typegoose": "^7.1.28",
    "nodemailer": "^6.4.10",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.3.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.14.0",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.3",
    "@types/node": "^14.0.14",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "@typescript-eslint/parser": "3.5.0",
    "eslint": "7.4.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "26.1.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.1",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@libs/db/(.*)": "<rootDir>/libs/db/src/$1",
      "@libs/db": "<rootDir>/libs/db/src",
      "@app/common/(.*)": "<rootDir>/libs/common/src/$1",
      "@app/common": "<rootDir>/libs/common/src",
      "@app/role/(.*)": "<rootDir>/libs/role/src/$1",
      "@app/role": "<rootDir>/libs/role/src",
      "@app/services/(.*)": "<rootDir>/libs/services/src/$1",
      "@app/services": "<rootDir>/libs/services/src",
      "@app/mail-queue/(.*)": "<rootDir>/libs/mail-queue/src/$1",
      "@app/mail-queue": "<rootDir>/libs/mail-queue/src"
    }
  }
}
